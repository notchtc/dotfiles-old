set scrollbar-bg \#4b565c
set scrollbar-fg \#d3c6aa

set statusbar-bg \#4b565c
set statusbar-fg \#d3c6aa

set inputbar-bg \#4b565c
set inputbar-fg \#a7c080

set completion-bg \#4b565c
set completion-fg \#d3c6aa
